{
  "name": "fumadocs-docgen",
  "version": "1.1.0",
  "description": "Useful remark utilities and plugins",
  "keywords": [
    "NextJs",
    "fumadocs",
    "remark",
    "mdx",
    "Docs"
  ],
  "homepage": "https://fumadocs.vercel.app",
  "repository": "github:fuma-nama/fumadocs",
  "license": "MIT",
  "author": "Fuma Nama",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist",
    "dev": "tsup --watch",
    "lint": "eslint .",
    "types:check": "tsc --noEmit"
  },
  "dependencies": {
    "@maximai/fumadocs-typescript": "workspace:^",
    "estree-util-value-to-estree": "^3.1.2",
    "hast-util-to-estree": "^3.1.0",
    "npm-to-yarn": "^2.2.1",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@mdx-js/mdx": "^3.0.1",
    "@types/estree": "^1.0.5",
    "@types/hast": "^3.0.4",
    "@types/mdast": "^4.0.3",
    "@types/node": "20.14.11",
    "eslint-config-custom": "workspace:*",
    "remark": "^15.0.0",
    "tsconfig": "workspace:*",
    "unified": "^11.0.5"
  },
  "publishConfig": {
    "access": "public"
  }
}
